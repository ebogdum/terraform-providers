output "documentation_part_id" {
  description = "The DocumentationPart identifier, generated by API Gateway when the documentation part is created."
  value       = aws_api_gateway_documentation_part.this.documentation_part_id
}

output "id" {
  description = "Unique ID of the Documentation Part"
  value       = aws_api_gateway_documentation_part.this.id
}

output "region" {
  description = "Region where this resource is managed"
  value       = aws_api_gateway_documentation_part.this.region
}

output "location" {
  description = "Location of the targeted API entity of the documentation part"
  value       = aws_api_gateway_documentation_part.this.location
}

output "location_type" {
  description = "Type of API entity to which the documentation content applies"
  value       = aws_api_gateway_documentation_part.this.location[0].type
}

output "location_method" {
  description = "HTTP verb of a method"
  value       = aws_api_gateway_documentation_part.this.location[0].method
}

output "location_name" {
  description = "Name of the targeted API entity"
  value       = aws_api_gateway_documentation_part.this.location[0].name
}

output "location_path" {
  description = "URL path of the target"
  value       = aws_api_gateway_documentation_part.this.location[0].path
}

output "location_status_code" {
  description = "HTTP status code of a response"
  value       = aws_api_gateway_documentation_part.this.location[0].status_code
}

output "properties" {
  description = "Content map of API-specific key-value pairs describing the targeted API entity"
  value       = aws_api_gateway_documentation_part.this.properties
}

output "rest_api_id" {
  description = "ID of the associated Rest API"
  value       = aws_api_gateway_documentation_part.this.rest_api_id
}